{
  "name": "meditify",
  "version": "1.0.0",
  "author": "Dario Ghirardo",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "test:develop": "ng test --browsers ChromeHeadless --watch=true",
    "test:debug": "ng test --browsers ChromeWithDebugging --watch=true",
    "lint": "eslint src/app",
    "lint:fix": "prettier --write . && eslint src/app --fix",
    "cypress:open": "cypress open",
    "compodoc": "compodoc -p tsconfig.doc.json -w -s",
    "docs:json": "compodoc -p ./tsconfig.doc.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "copy:ios": "ionic capacitor copy ios",
    "copy:android": "ionic capacitor copy android",
    "open:ios": "ionic capacitor run ios -l --external",
    "open:android": "ionic capacitor run android -l --external"
  },
  "private": true,
  "dependencies": {
